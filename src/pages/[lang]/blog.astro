---
import BlogPage from "../../components/BlogPage.astro";
import { languages } from "../../i18n/ui";

export function getStaticPaths() {
    return Object.keys(languages)
        .filter((l) => l !== "en")
        .map((lang) => ({ params: { lang } }));
}

const { lang } = Astro.params;
---

<BlogPage lang={lang} />
